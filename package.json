{
  "name": "bob-rockefeller",
  "description": "What is Bob up to now?",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Bob Rockefeller",
  "keywords": [
    "hugo",
    "tailwindcss",
    "tailwindcss-v4"
  ],
  "scripts": {
    "serve-d": "rm -rf public && rm -rf resources && hugo server -D",
    "serve": "rm -rf public && rm -rf resources && hugo server",
    "build": "hugo --gc --minify",
    "watch": "hugo server --disableFastRender --navigateToChanged",
    "clean": "rm -rf public && rm -rf resources"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.1",
    "@tailwindcss/typography": "^0.5.16",
    "flowbite": "^3.1.2",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.1"
  }
}
